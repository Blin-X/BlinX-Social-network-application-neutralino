{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.blinx.app",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.exit",
    "window.setTitle",
    "window.maximize",
    "window.minimize",
    "window.setFullScreen",
    "window.unFullScreen",
    "os.getEnv",
    "debug.log"
  ],
  "globalVariables": {
    "APP_MODE": "desktop",
    "SITE_URL": "http://localhost/app"
  },
  "modes": {
    "window": {
      "title": "Blinx",
      "width": 1400,
      "height": 900,
      "minWidth": 400,
      "minHeight": 300,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": false,
      "borderless": false,
      "maximize": true,
      "hidden": false,
      "resizable": true,
      "center": true,
      "exitProcessOnClose": true,
      "icon": "/resources/icon.ico"
    }
  },
  "cli": {
    "binaryName": "blinx-app",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.9.0",
    "clientVersion": "5.9.0"
  }
}